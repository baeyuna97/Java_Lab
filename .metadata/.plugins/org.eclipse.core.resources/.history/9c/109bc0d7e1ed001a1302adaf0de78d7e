package classpart;

public class Student {
	// 클래스 생성  
	
	int studentID;
	String studentName;
	int grade;
	String address;  
	
	// 메소드 생성
	public void showStudentInfo() {
		System.out.println(studentName + "," + address);
	}
	// 시작하는 코드 
	// 클래스 돌리는 Main method
	public String getStudentName() {
		return studentName; 
	}
	
	public void setStudentName(String name) {
		studentName = name;
	}
}




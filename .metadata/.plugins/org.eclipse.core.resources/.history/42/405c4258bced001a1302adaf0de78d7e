package binary;

public class ExplicitConversion {

	public static void main(String[] args) {
		
		int iNum =1000;
		byte bNum = (byte)iNum;
		// type cast
		// byte데이터는 1바이트만 사용 -128 ~ 127 
		// 1000 여기서 못 사용 
		
		System.out.println(iNum);
		System.out.println(bNum);
		// 큰 수에서 작은 수로 넣다보면 값이 유실될 수 있다. 
		
	}

}

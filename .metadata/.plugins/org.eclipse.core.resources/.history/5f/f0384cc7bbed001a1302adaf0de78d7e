package binary;

public class ImplicitConversion {

	public static void main(String[] args) {
		byte bNum = 10;
		int num = bNum; // 4바이트 
		// bNum 1바이트 

		
		System.out.println(num);
		
		long lNum = 10; // literal -> 4byte 저장 
		float fNum = lNum; // long이라는 8바이트에 저장 -> 형변환
		
		System.out.println(fNum);
	}

}
